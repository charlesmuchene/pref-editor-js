{
    "branches": [
        "main"
    ],
    "plugins": [
        [
            "@semantic-release/changelog",
            {
                "changelogFile": "CHANGELOG.md"
            }
        ],
        [
            "@semantic-release/git",
            {
                "assets": [
                    "CHANGELOG.md"
                ],
                "message": "chore(release): update changelog [skip ci]"
            }
        ]
    ]
}