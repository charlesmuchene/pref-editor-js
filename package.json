{
  "name": "@charlesmuchene/pref-editor",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "builder": "tsc",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src/ --ext .ts",
    "build": "npm run compile-protos && npm run copy-protos && tsc",
    "clean": "rimraf index.d.ts src/**/*.js dist src/**/*.d.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-protos": "mkdir -p dist/protos && cp src/protos/message.js dist/protos/message.js && cp src/protos/message.d.ts dist/protos/message.d.ts",
    "compile-protos": "pbjs -t static-module -w commonjs -o src/protos/message.js src/protos/message.proto && pbts -o src/protos/message.d.ts src/protos/message.js",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "shared preferences",
    "datastore",
    "android"
  ],
  "author": {
    "name": "Charles Muchene",
    "url": "https://charlesmuchene.com"
  },
  "license": "Apache-2.0",
  "description": "View and Edit Android preferences ",
  "repository": {
    "type": "git",
    "url": "https://github.com/charlesmuchene/pref-editor-js.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/node": "^22.15.0",
    "eslint": "^9.25.1",
    "protobufjs-cli": "^1.1.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  },
  "dependencies": {
    "@devicefarmer/adbkit": "^3.3.8",
    "protobufjs": "^7.5.0"
  }
}